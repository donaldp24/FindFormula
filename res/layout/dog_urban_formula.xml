<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_dog_urban_formula"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for formula ////////////////////////////// -->
	<!-- title -->
	<ImageView
			android:layout_marginLeft="385px"
			android:layout_marginTop="64px"
			android:layout_width="519px"
			android:layout_height="50px"
			android:background="@drawable/cat_constant_formula_title_lifestyle"
			/>

	<!-- back button -->
	<Button
			android:id="@+id/dog_urban_formula_btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="270px"
			android:layout_height="80px"
			android:background="@drawable/cat_breed_btn_back"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="118px"
			android:layout_marginTop="702px"
			android:layout_width="1073px"
			android:layout_height="68px"
			android:background="@drawable/dog_urban_sel"
			/>
	<Button
			android:id="@+id/dog_btn_urban"
			android:layout_marginLeft="80px"
			android:layout_marginTop="670px"
			android:layout_width="330px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_indoor"
			android:layout_marginLeft="430px"
			android:layout_marginTop="670px"
			android:layout_width="390px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_sporting"
			android:layout_marginLeft="880px"
			android:layout_marginTop="670px"
			android:layout_width="370px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<!-- ////////////////// end constants //////////////////////// -->

	<!-- good -->
	<ImageView
			android:layout_marginLeft="148px"
			android:layout_marginTop="136px"
			android:layout_width="976px"
			android:layout_height="513px"
			android:background="@drawable/dog_urban_formula"
			/>
	<Button
			android:id="@+id/btn_junior"
			android:layout_marginLeft="148px"
			android:layout_marginTop="136px"
			android:layout_width="320px"
			android:layout_height="513px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/btn_adult"
			android:layout_marginLeft="490px"
			android:layout_marginTop="136px"
			android:layout_width="280px"
			android:layout_height="513px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/btn_senior"
			android:layout_marginLeft="775px"
			android:layout_marginTop="136px"
			android:layout_width="330px"
			android:layout_height="513px"
			android:background="@android:color/transparent"
			/>

</RelativeLayout>