<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_dog_giant_formula"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for formula ////////////////////////////// -->
	<!-- title -->
	<ImageView
			android:layout_marginLeft="385px"
			android:layout_marginTop="64px"
			android:layout_width="519px"
			android:layout_height="50px"
			android:background="@drawable/cat_constant_formula_title_lifestyle"
			/>

	<!-- back button -->
	<Button
			android:id="@+id/dog_giant_formula_btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="270px"
			android:layout_height="80px"
			android:background="@drawable/cat_breed_btn_back"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="82px"
			android:layout_marginTop="702px"
			android:layout_width="1125px"
			android:layout_height="45px"
			android:background="@drawable/dog_giant_sel"
			/>
	<Button
			android:id="@+id/dog_btn_xsmall"
			android:layout_marginLeft="50px"
			android:layout_marginTop="670px"
			android:layout_width="240px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_mini"
			android:layout_marginLeft="300px"
			android:layout_marginTop="670px"
			android:layout_width="240px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_medium"
			android:layout_marginLeft="560px"
			android:layout_marginTop="670px"
			android:layout_width="240px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_maxi"
			android:layout_marginLeft="810px"
			android:layout_marginTop="670px"
			android:layout_width="240px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_giant"
			android:layout_marginLeft="1050px"
			android:layout_marginTop="670px"
			android:layout_width="230px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<!-- ////////////////// end constants //////////////////////// -->

	<!-- good -->
	<ImageView
			android:layout_marginLeft="461px"
			android:layout_marginTop="141px"
			android:layout_width="368px"
			android:layout_height="534px"
			android:background="@drawable/dog_giant_formula"
			/>

</RelativeLayout>