<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_cat_wet_spots"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for spots ////////////////////////////// -->
	<!-- info spot explain -->
	<ImageView
			android:layout_marginLeft="34px"
			android:layout_marginTop="80px"
			android:layout_width="267px"
			android:layout_height="105px"
			android:background="@drawable/cat_constant_spot_explain"
			/>

	<!-- breed button -->
	<Button
			android:id="@+id/cat_wet_spots_btn_breed"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="125px"
			android:layout_height="113px"
			android:background="@drawable/cat_constant_btn_breed"
			/>
	<!-- lifestyle button -->
	<Button
			android:id="@+id/cat_wet_spots_btn_lifestyle"
			android:layout_marginLeft="125px"
			android:layout_marginTop="0px"
			android:layout_width="204px"
			android:layout_height="113px"
			android:background="@drawable/cat_constant_btn_lifestyle"
			/>
	<!-- find formula button -->
	<Button
			android:id="@+id/cat_wet_spots_btn_findformula"
			android:layout_marginLeft="1016px"
			android:layout_marginTop="13px"
			android:layout_width="264px"
			android:layout_height="116px"
			android:background="@drawable/cat_constant_btn_findformula"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="59px"
			android:layout_marginTop="690px"
			android:layout_width="1170px"
			android:layout_height="77px"
			android:background="@drawable/cat_wet_sel"
			/>
	<Button
			android:id="@+id/cat_btn_kitten"
			android:layout_marginLeft="50px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_indoor"
			android:layout_marginLeft="310px"
			android:layout_marginTop="700px"
			android:layout_width="260px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_spayed"
			android:layout_marginLeft="570px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_special"
			android:layout_marginLeft="830px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_wet"
			android:layout_marginLeft="1070px"
			android:layout_marginTop="700px"
			android:layout_width="230px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<!-- ////////////////// end constants //////////////////////// -->


	<!-- food -->
	<Button
			android:id="@+id/cat_wet_spots_btn_food"
			android:layout_marginLeft="334px"
			android:layout_marginTop="141px"
			android:layout_width="453px"
			android:layout_height="180px"
			android:background="@drawable/cat_wet_food"
			/>


	<!-- cat -->
	<ImageView
			android:layout_marginLeft="113px"
			android:layout_marginTop="126px"
			android:layout_width="1084px"
			android:layout_height="540px"
			android:background="@drawable/cat_wet_cat"
			/>

	<!-- spot buttons -->
	<Button
			android:id="@+id/cat_wet_spots_btn_spot2"
			android:layout_marginLeft="234px"
			android:layout_marginTop="430px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/cat_wet_spots_btn_spot4"
			android:layout_marginLeft="670px"
			android:layout_marginTop="410px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/cat_wet_spots_btn_spot1"
			android:layout_marginLeft="830px"
			android:layout_marginTop="420px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/cat_wet_spots_btn_spot3"
			android:layout_marginLeft="900px"
			android:layout_marginTop="423px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/cat_wet_spots_btn_spot5"
			android:layout_marginLeft="1023px"
			android:layout_marginTop="500px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

</RelativeLayout>