<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_dog_sporting_spots"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for spots ////////////////////////////// -->
	<!-- info spot explain -->
	<ImageView
			android:layout_marginLeft="34px"
			android:layout_marginTop="80px"
			android:layout_width="267px"
			android:layout_height="105px"
			android:background="@drawable/cat_constant_spot_explain"
			/>

	<!-- home button -->
	<Button
			android:id="@+id/dog_home"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="224px"
			android:layout_height="105px"
			android:background="@drawable/dog_constant_home"
			/>
	<!-- find formula button -->
	<Button
			android:id="@+id/dog_sporting_spots_btn_findformula"
			android:layout_marginLeft="1016px"
			android:layout_marginTop="13px"
			android:layout_width="264px"
			android:layout_height="116px"
			android:background="@drawable/cat_constant_btn_findformula"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="118px"
			android:layout_marginTop="702px"
			android:layout_width="1073px"
			android:layout_height="68px"
			android:background="@drawable/dog_sporting_sel"
			/>
	<Button
			android:id="@+id/dog_btn_urban"
			android:layout_marginLeft="80px"
			android:layout_marginTop="670px"
			android:layout_width="330px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_indoor"
			android:layout_marginLeft="430px"
			android:layout_marginTop="670px"
			android:layout_width="390px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/dog_btn_sporting"
			android:layout_marginLeft="880px"
			android:layout_marginTop="670px"
			android:layout_width="370px"
			android:layout_height="90px"
			android:background="@android:color/transparent"
			/>
	<!-- ////////////////// end constants //////////////////////// -->


	<!-- food -->
	<Button
			android:id="@+id/dog_sporting_spots_btn_food"
			android:layout_marginLeft="73px"
			android:layout_marginTop="380px"
			android:layout_width="233px"
			android:layout_height="225px"
			android:background="@drawable/dog_indoor_food"
			/>


	<!-- dog -->
	<ImageView
			android:layout_marginLeft="450px"
			android:layout_marginTop="25px"
			android:layout_width="445px"
			android:layout_height="629px"
			android:background="@drawable/dog_sporting_dog"
			/>

	<!-- spot buttons -->
	<Button
			android:id="@+id/dog_sporting_spots_btn_spot4"
			android:layout_marginLeft="580px"
			android:layout_marginTop="470px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/dog_sporting_spots_btn_spot3"
			android:layout_marginLeft="635px"
			android:layout_marginTop="290px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/dog_sporting_spots_btn_spot1"
			android:layout_marginLeft="630px"
			android:layout_marginTop="60px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/dog_sporting_spots_btn_spot2"
			android:layout_marginLeft="740px"
			android:layout_marginTop="200px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

</RelativeLayout>