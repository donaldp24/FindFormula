<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_cat_kitten_formula"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for formula ////////////////////////////// -->
	<!-- title -->
	<ImageView
			android:layout_marginLeft="385px"
			android:layout_marginTop="64px"
			android:layout_width="519px"
			android:layout_height="50px"
			android:background="@drawable/cat_constant_formula_title_lifestyle"
			/>

	<!-- back button -->
	<Button
			android:id="@+id/cat_kitten_formula_btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="270px"
			android:layout_height="80px"
			android:background="@drawable/cat_breed_btn_back"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="59px"
			android:layout_marginTop="690px"
			android:layout_width="1170px"
			android:layout_height="77px"
			android:background="@drawable/cat_kitten_sel"
			/>
	<!-- ////////////////// end constants //////////////////////// -->

	<!-- good -->
	<ImageView
			android:layout_marginLeft="461px"
			android:layout_marginTop="141px"
			android:layout_width="368px"
			android:layout_height="534px"
			android:background="@drawable/cat_kitten_formula"
			/>

</RelativeLayout>