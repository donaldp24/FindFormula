			/>

	<Button
			android:id="@+id/dog_labrador_spots_btn_spot3"
			android:layout_marginLeft="510px"
			android:layout_marginTop="124px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

	<Button
			android:id="@+id/dog_labrador_spots_btn_spot4"
			android:layout_marginLeft="810px"
			android:layout_marginTop="510px"
			android:layout_width="110px"
			android:layout_height="110px"
			android:background="@drawable/cat_constant_btn_spot"
			/>

</RelativeLayout>