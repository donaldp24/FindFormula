	<Button
			android:id="@+id/dog_labrador_spots_btn_food"
			android:layout_marginLeft="97px"
			android:layout_marginTop="405px"
			android:layout_width="160px"
			android:layout_height="200px"
			android:background="@drawable/dog_labrador_food"
			/>


