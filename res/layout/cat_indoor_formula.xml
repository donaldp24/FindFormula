<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_cat_indoor_formula"
                android:orientation="vertical"
                android:layout_width="1280px"
                android:layout_height="800px"
                android:background="@android:color/white">

	<!-- ///////////////////////////// constants for formula ////////////////////////////// -->
	<!-- title -->
	<ImageView
			android:layout_marginLeft="385px"
			android:layout_marginTop="64px"
			android:layout_width="519px"
			android:layout_height="50px"
			android:background="@drawable/cat_constant_formula_title_lifestyle"
			/>

	<!-- back button -->
	<Button
			android:id="@+id/cat_indoor_formula_btn_back"
			android:layout_marginLeft="0px"
			android:layout_marginTop="0px"
			android:layout_width="270px"
			android:layout_height="80px"
			android:background="@drawable/cat_breed_btn_back"
			/>

	<!-- image bar -->
	<ImageView
			android:layout_marginLeft="0px"
			android:layout_marginTop="673px"
			android:layout_width="1280px"
			android:layout_height="8px"
			android:background="@drawable/cat_constant_img_bar_lifestyle"
			/>

	<!-- select explain img -->
	<ImageView
			android:layout_marginLeft="59px"
			android:layout_marginTop="690px"
			android:layout_width="1170px"
			android:layout_height="77px"
			android:background="@drawable/cat_indoor_sel"
			/>
	<Button
			android:id="@+id/cat_btn_kitten"
			android:layout_marginLeft="50px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_indoor"
			android:layout_marginLeft="290px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_spayed"
			android:layout_marginLeft="570px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_special"
			android:layout_marginLeft="830px"
			android:layout_marginTop="700px"
			android:layout_width="240px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<Button
			android:id="@+id/cat_btn_wet"
			android:layout_marginLeft="1070px"
			android:layout_marginTop="700px"
			android:layout_width="230px"
			android:layout_height="58px"
			android:background="@android:color/transparent"
			/>
	<!-- ////////////////// end constants //////////////////////// -->

	<!-- good -->
	<ImageView
			android:layout_marginLeft="41px"
			android:layout_marginTop="163px"
			android:layout_width="1194px"
			android:layout_height="494px"
			android:background="@drawable/cat_indoor_formula"
			/>

</RelativeLayout>